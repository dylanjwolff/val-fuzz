(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const x Int)
(declare-const y Int)
(assert (and (= (+ x y) GEN1) (< x (- GEN2))))
(assert (and (and (= BAV3 (and (= (+ x y) GEN1) (< x (- GEN2)))) (= BAV4 (= (+ x y) GEN1))) (= BAV5 (< x (- GEN2)))))
(assert (and (and (= BAV3 false) (= BAV4 true)) (= BAV5 true)))
(check-sat)
(get-model)