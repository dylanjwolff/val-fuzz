(set-logic QF_UFNIA)
(declare-fun a () Int)
(declare-fun b (Int) Bool)
(declare-fun c (Int) Int)
(declare-fun d (Int) Bool)
(declare-fun i (Int) Bool)
(define-fun e ((f Int)) Bool (= (b f) (ite (= f a) (= 0 (div (c f) 2)) true)))
(define-fun g ((f Int)) Bool (= (c f) (ite (= f a) 0 (ite (i f) 0 (ite (d f) 0 (c 0))))))
(define-fun h ((f Int)) Bool (= (b f) true))
(assert (e 0))
(assert (g 0))
(assert (= a 0))
(assert (not (h 0)))
(check-sat)
