(declare-const GEN1 String)
(declare-const GEN2 String)
(declare-fun a () String)
(declare-const BAV1 String)
(declare-const BAV2 String)
(declare-const BAV3 Bool)
(declare-const BDOM1 Bool)
(declare-const BDOM2 Bool)
(declare-const BDOM3 Bool)
(assert (= BAV1 (str.++ a GEN1)))
(assert (= BAV2 (str.++ GEN2 a)))
(assert (= BAV3 (distinct (str.++ a GEN1) (str.++ GEN2 a))))
(assert (= (= BAV1 "") BDOM1))
(assert (= (= BAV2 "") BDOM2))
(assert (= BDOM3 BAV3))
(check-sat)
(get-model)
