(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-fun A () (Set Int))
(declare-fun C () (Set Int))
(declare-fun D () (Set Int))
(declare-fun E () (Set Int))
(assert (= A (union D C)))
(assert (not (= A (union E A))))
(assert (= BAV1 (= A (union D C))))
(assert (= BAV2 (= A (union E A))))
(check-sat)
(get-model)