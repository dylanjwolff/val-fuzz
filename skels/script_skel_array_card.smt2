(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(set-option :produce-models true)
(declare-sort U 0)
(declare-fun f () (Array U U))
(declare-fun a () U)
(declare-fun b () U)
(declare-fun c () U)
(assert (distinct a b c))
(assert (distinct (select f a) (select f b)))
(assert (forall ((x U)) (or (= (select f x) c) (= (select f x) b))))
(assert (= BAV1 (distinct a b c)))
(assert (= BAV2 (distinct (select f a) (select f b))))
(assert (= BAV3 (forall ((x U)) (or (= (select f x) c) (= (select f x) b)))))
(assert (= BAV4 (forall ((x U)) (= (select f x) c))))
(assert (= BAV5 (forall ((x U)) (= (select f x) b))))
(check-sat)
(get-model)