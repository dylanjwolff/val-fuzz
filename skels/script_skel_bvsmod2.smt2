(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN6 (_ BitVec 2))
(declare-const GEN5 (_ BitVec 2))
(get-model)
(declare-const GEN4 (_ BitVec 2))
(declare-const GEN3 (_ BitVec 2))
(declare-const GEN2 (_ BitVec 2))
(declare-const GEN1 (_ BitVec 2))
(set-option :auto-config true)
(define-fun bvsmod_def ((s (_ BitVec 2)) (t (_ BitVec 2))) (_ BitVec 2)
      (let ((msb_s ((_ extract 1 1) s))
            (msb_t ((_ extract 1 1) t)))
        (let ((abs_s (ite (= msb_s #b0) s (bvneg s)))
              (abs_t (ite (= msb_t #b0) t (bvneg t))))
          (let ((u (bvurem abs_s abs_t)))
            (ite (= u (_ bv0 2))
                 u
            (ite (and (= msb_s #b0) (= msb_t #b0))
                 u
            (ite (and (= msb_s #b1) (= msb_t #b0))
                 (bvadd (bvneg u) t)
            (ite (and (= msb_s #b0) (= msb_t #b1))
                 (bvadd u t)
                 (bvneg u)))))))))
(declare-const a (_ BitVec 2))
(push)
(assert (not (= (bvsmod_def a GEN1) (ite (bvslt a GEN2) GEN3 GEN4))))
(assert (= BAV1 (= (bvsmod_def a GEN1) (ite (bvslt a GEN2) GEN3 GEN4))))
(assert (= BAV2 (= (bvsmod_def a GEN5) GEN6)))
(check-sat)
(get-model)
(pop)
(assert (not (= (bvsmod_def a GEN5) GEN6)))
(check-sat)