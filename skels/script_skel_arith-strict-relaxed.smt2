(set-logic ALL)
(declare-const GEN1 Int)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-fun x () Real)
(declare-fun y () Real)
(declare-fun z () Real)
(assert (< y GEN1))
(assert (>= y x))
(assert (>= y (- x)))
(assert (= BAV2 (< y GEN1)))
(assert (= BAV3 (>= y x)))
(assert (= BAV4 (>= y (- x))))
(check-sat)