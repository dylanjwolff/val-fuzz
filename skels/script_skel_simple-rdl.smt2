(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN1 Int)
(declare-fun x () Real)
(declare-fun y () Real)
(assert (not (=> (< (- x y) GEN1) (< x y))))
(assert (= BAV1 (=> (< (- x y) GEN1) (< x y))))
(assert (= BAV2 (< (- x y) GEN1)))
(assert (= BAV3 (< x y)))
(check-sat)
(get-model)