(set-logic ALL)
(declare-const GEN1 Int)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-fun x () Real)
(declare-fun y () Real)
(assert (not (=> (< (- x y) GEN1) (< x y))))
(assert (= BAV2 (=> (< (- x y) GEN1) (< x y))))
(assert (= BAV3 (< (- x y) GEN1)))
(assert (= BAV4 (< x y)))
(check-sat)