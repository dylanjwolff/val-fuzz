(declare-const BAV1 Bool)
(declare-const GEN5 (_ BitVec 32))
(declare-const GEN4 (_ BitVec 32))
(declare-const GEN3 (_ BitVec 32))
(declare-const GEN2 (_ BitVec 32))
(declare-const GEN1 (_ BitVec 32))
(set-option :produce-models true)
(declare-const a (_ BitVec 32))
(declare-const b (_ BitVec 32))
(assert (bvsle (bvneg GEN1) a))
(assert (bvsle a GEN2))
(assert (= (bvadd a b) GEN3))
(apply (and-then simplify reduce-bv-size) :print-model-converter true)
(assert (bvsle (bvneg GEN4) b))
(assert (bvsle b GEN5))
(apply (and-then simplify reduce-bv-size) :print-model-converter true)
(assert (= BAV1 (= (bvadd a b) GEN3)))