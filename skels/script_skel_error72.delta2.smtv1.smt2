(set-option :incremental false)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun v1 () Int)
(assert (= BAV1 (>= (- (ite (distinct v1 GEN1) v1 GEN2)) GEN3)))
(assert (= BAV2 (distinct v1 GEN1)))
(check-sat-assuming ((>= (- (ite (distinct v1 GEN1) v1 GEN2)) GEN3)))
(get-model)