(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () Int)
(declare-fun y () Int)
(assert (> x GEN1))
(assert (> y GEN2))
(assert (not (= (div (* x y) y) x)))
(assert (= BAV1 (> x GEN1)))
(assert (= BAV2 (> y GEN2)))
(assert (= BAV3 (= (div (* x y) y) x)))
(check-sat)
(get-model)