(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN1 Int)
(declare-fun x () Real)
(declare-fun y () Real)
(declare-fun z () Real)
(assert (> z GEN1))
(assert (> x y))
(assert (< (* x z) (* y z)))
(assert (= BAV1 (> z GEN1)))
(assert (= BAV2 (> x y)))
(assert (= BAV3 (< (* x z) (* y z))))
(check-sat)
(get-model)