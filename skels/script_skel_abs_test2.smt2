(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-fun x () Int)
(assert (>= x GEN1))
(assert (> (abs x) GEN2))
(assert (= BAV3 (>= x GEN1)))
(assert (= BAV4 (> (abs x) GEN2)))
(check-sat)
(get-model)
(get-value (x (abs x)))