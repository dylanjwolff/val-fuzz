(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const GEN3 Int)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(declare-fun i () Int)
(declare-fun j () Int)
(assert (> (+ i j) GEN1))
(assert (< i GEN2))
(assert (< j GEN3))
(assert (= BAV4 (> (+ i j) GEN1)))
(assert (= BAV5 (< i GEN2)))
(assert (= BAV6 (< j GEN3)))
(check-sat)