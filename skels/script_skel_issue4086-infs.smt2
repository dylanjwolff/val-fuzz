(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN1 Int)
(set-option :cbqi-use-inf-int true)
(set-option :cbqi-use-inf-real true)
(set-option :var-ineq-elim-quant false)
(assert (forall ((b Real)) (forall ((c Int)) (and (> c (* b GEN1))))))
(assert (= BAV1 (forall ((b Real)(c Int)) (and (> c (* b GEN1))))))
(assert (= BAV2 (forall ((b Real)(c Int)) (> c (* b GEN1)))))
(check-sat)
(get-model)