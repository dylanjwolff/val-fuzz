(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const GEN3 Int)
(declare-const GEN4 Int)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(declare-const BAV7 Bool)
(declare-const BAV8 Bool)
(declare-fun x () Real)
(declare-fun P () Bool)
(assert (and (not (= (ite P GEN1 x) GEN2)) (> (ite P GEN1 x) GEN3) (<= (ite P GEN1 x) GEN4)))
(assert (= BAV5 (and (not (= (ite P GEN1 x) GEN2)) (> (ite P GEN1 x) GEN3) (<= (ite P GEN1 x) GEN4))))
(assert (= BAV6 (= (ite P GEN1 x) GEN2)))
(assert (= BAV7 (> (ite P GEN1 x) GEN3)))
(assert (= BAV8 (<= (ite P GEN1 x) GEN4)))
(check-sat)
(get-model)
(exit)