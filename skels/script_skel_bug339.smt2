(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const GEN4 Int)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () Real)
(declare-fun P () Bool)
(assert (and (not (= (ite P GEN1 x) GEN2)) (> (ite P GEN1 x) GEN3) (<= (ite P GEN1 x) GEN4)))
(assert (= BAV1 (and (not (= (ite P GEN1 x) GEN2)) (> (ite P GEN1 x) GEN3) (<= (ite P GEN1 x) GEN4))))
(assert (= BAV2 (= (ite P GEN1 x) GEN2)))
(assert (= BAV3 (> (ite P GEN1 x) GEN3)))
(assert (= BAV4 (<= (ite P GEN1 x) GEN4)))
(check-sat)
(get-model)
(exit)