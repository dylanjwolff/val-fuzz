(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(declare-sort U 0)
(declare-fun x () U)
(declare-fun y () U)
(declare-fun z () U)
(declare-fun w () U)
(declare-fun u () U)
(declare-fun v () U)
(declare-fun a () U)
(declare-fun P (U) Bool)
(assert (distinct w u v a))
(assert (or (= x y) (= x z) (= y z)))
(assert (or (P x) (P y) (P z) (P w)))
(assert (= BAV1 (distinct w u v a)))
(assert (= BAV2 (or (= x y) (= x z) (= y z))))
(assert (= BAV3 (= x y)))
(assert (= BAV4 (= x z)))
(assert (= BAV5 (= y z)))
(assert (= BAV6 (or (P x) (P y) (P z) (P w))))
(check-sat)
(get-model)