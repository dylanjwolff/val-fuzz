(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const GEN7 Int)
(declare-const GEN6 Int)
(declare-const GEN5 Int)
(declare-const GEN4 Int)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(set-info :source | SMT-COMP'06 organizers |)
(set-info :smt-lib-version 2.0)
(set-info :category "check")
(set-info :notes |This benchmark is designed to check if the DP supports bignumbers.|)
(declare-fun f (Int) Int)
(assert (= (f GEN1) GEN2))
(assert (forall ((?x Int)) (=> (> ?x GEN3) (= (f ?x) (* (- GEN4) (f (- ?x GEN5)))))))
(assert (< (f GEN6) GEN7))
(assert (= BAV1 (= (f GEN1) GEN2)))
(assert (= BAV2 (forall ((?x Int)) (=> (> ?x GEN3) (= (f ?x) (* (- GEN4) (f (- ?x GEN5))))))))
(assert (= BAV3 (forall ((?x Int)) (> ?x GEN3))))
(assert (= BAV4 (forall ((?x Int)) (= (f ?x) (* (- GEN4) (f (- ?x GEN5)))))))
(assert (= BAV5 (< (f GEN6) GEN7)))
(check-sat)
(get-model)
(exit)