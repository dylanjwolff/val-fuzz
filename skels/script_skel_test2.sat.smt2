(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN1 Int)
(declare-fun x () Real)
(assert (forall ((y Real)) (=> (> y GEN1) (>= y x))))
(assert (= BAV1 (forall ((y Real)) (=> (> y GEN1) (>= y x)))))
(assert (= BAV2 (forall ((y Real)) (> y GEN1))))
(assert (= BAV3 (forall ((y Real)) (>= y x))))
(check-sat)
(get-model)