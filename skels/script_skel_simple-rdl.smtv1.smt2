(set-option :incremental false)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN1 Real)
(declare-fun x () Real)
(declare-fun y () Real)
(assert (= BAV1 (=> (< (- x y) GEN1) (< x y))))
(assert (= BAV2 (< (- x y) GEN1)))
(assert (= BAV3 (< x y)))
(check-sat-assuming ((not (=> (< (- x y) GEN1) (< x y)))))
(get-model)