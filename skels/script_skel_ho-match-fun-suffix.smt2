(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun f (Int Int) Int)
(declare-fun a () Int)
(declare-fun b () Int)
(assert (forall ((x (-> Int Int))(y Int)) (not (= (x y) GEN1))))
(assert (= (f a b) GEN2))
(assert (= BAV1 (forall ((x (-> Int Int))(y Int)) (= (x y) GEN1))))
(assert (= BAV2 (= (f a b) GEN2)))
(check-sat)
(get-model)