(set-logic ALL)
(declare-const GEN1 (_ BitVec 1))
(declare-const GEN2 (_ BitVec 8))
(declare-const GEN3 (_ BitVec 23))
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(set-info :source "Handcrafted by CM Wintersteiger")
(define-sort FPN () (_ FloatingPoint 8 24))
(declare-fun x () FPN)
(declare-fun r () FPN)
(declare-fun m () FPN)
(assert (= x (fp GEN1 GEN2 GEN3)))
(assert (= r (fp.roundToIntegral RNE x)))
(assert (= BAV4 (= x (fp GEN1 GEN2 GEN3))))
(assert (= BAV5 (= r (fp.roundToIntegral RNE x))))
(check-sat)
(get-value (x))
(get-value (r))
(check-sat-using (then
                     fpa2bv
                     (using-params simplify :elim_and true)
                     bit-blast
                     sat))
(get-value (r))
(exit)