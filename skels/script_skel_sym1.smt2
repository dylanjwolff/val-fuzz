(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-fun x () Int)
(declare-fun y () Int)
(declare-fun z () Int)
(declare-fun p (Int) Bool)
(declare-fun A () (Set Int))
(declare-fun F () (Set Int))
(assert (= F (insert x y (singleton z))))
(assert (subset F A))
(assert (= x y))
(assert (= BAV1 (= F (insert x y (singleton z)))))
(assert (= BAV2 (= x y)))
(check-sat)
(get-model)