(declare-sort A 0)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-fun e (A) A)
(declare-fun c (A A) A)
(declare-fun h (A A) A)
(declare-fun b (A) Bool)
(declare-fun d (A) Bool)
(assert (not (forall ((f A)(i A)) (distinct (h f i) (e (c f i))))))
(assert (and (forall ((j A)) (or (not (b j)) (d (e j)))) (forall ((i A)) (b i)) (forall ((g A)) (not (b g)))))
(assert (= BAV1 (forall ((f A)(i A)) (distinct (h f i) (e (c f i))))))
(assert (= BAV2 (and (forall ((j A)) (or (not (b j)) (d (e j)))) (forall ((i A)) (b i)) (forall ((g A)) (not (b g))))))
(assert (= BAV3 (forall ((j A)) (or (not (b j)) (d (e j))))))
(check-sat)