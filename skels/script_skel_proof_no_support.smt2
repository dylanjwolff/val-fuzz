(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(declare-const a Int)
(assert (and (= a (* a a)) (not (= a GEN1)) (not (= a GEN2))))
(assert (= BAV3 (and (= a (* a a)) (not (= a GEN1)) (not (= a GEN2)))))
(assert (= BAV4 (= a (* a a))))
(assert (= BAV5 (= a GEN1)))
(assert (= BAV6 (= a GEN2)))
(check-sat)