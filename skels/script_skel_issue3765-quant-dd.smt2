(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-sort U 0)
(declare-fun g (U) Int)
(declare-sort V 0)
(declare-fun f (V) Int)
(assert (and (forall ((?i U)) (not (forall ((?z V)) (not (= (f ?z) (div (- GEN1) (g ?i)))))))))
(declare-fun k () U)
(assert (= (g k) GEN2))
(assert (= BAV1 (and (forall ((?i U)) (not (forall ((?z V)) (not (= (f ?z) (div (- GEN1) (g ?i))))))))))
(assert (= BAV2 (forall ((?i U)(?z V)) (= (f ?z) (div (- GEN1) (g ?i))))))
(assert (= BAV3 (= (g k) GEN2)))
(check-sat)
(get-model)