(set-logic ALL)
(declare-const BAV1 Bool)
(set-option :produce-models true)
(declare-fun f (Bool) Bool)
(declare-fun x () Bool)
(declare-fun y () Bool)
(declare-fun z () Bool)
(assert (or (f x) (f y) (f z)))
(assert (not (f false)))
(assert (not (f true)))
(assert (= BAV1 (or (f x) (f y) (f z))))
(check-sat)