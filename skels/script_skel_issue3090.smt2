(set-logic ALL)
(declare-const GEN1 String)
(declare-const GEN2 String)
(declare-const GEN3 String)
(declare-const BAV4 Bool)
(set-option :strings-exp true)
(declare-const id String)
(assert (and (str.in.re id (re.+ (re.range GEN1 GEN2))) (str.contains id GEN3)))
(assert (= BAV4 (and (str.in.re id (re.+ (re.range GEN1 GEN2))) (str.contains id GEN3))))
(check-sat)
(get-model)