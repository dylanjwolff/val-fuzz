(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const x Int)
(declare-const y Int)
(declare-const a Int)
(declare-const b Int)
(assert (and (sep (pto x a) (or (pto x a) (pto y b))) (sep (pto y b) (or (pto x a) (pto y b)))))
(assert (not (sep (pto x a) (pto y b))))
(assert (= BAV1 (and (sep (pto x a) (or (pto x a) (pto y b))) (sep (pto y b) (or (pto x a) (pto y b))))))
(assert (= BAV2 (or (pto x a) (pto y b))))
(assert (= BAV3 (or (pto x a) (pto y b))))
(check-sat)
(get-model)