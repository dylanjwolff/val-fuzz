(set-info :smt-lib-version 2.5)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN1 Int)
(set-option :strings-exp true)
(declare-datatypes () ((Val
    (Str (str String))
    (Num (num Int)))))
(declare-const var0 Val)
(assert (=> (is-Str var0) (distinct (str.to.int (str var0)) (- GEN1))))
(assert (= BAV1 (=> (is-Str var0) (distinct (str.to.int (str var0)) (- GEN1)))))
(assert (= BAV2 (distinct (str.to.int (str var0)) (- GEN1))))
(check-sat)
(get-model)