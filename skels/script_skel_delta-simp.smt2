(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun c () Real)
(assert (forall ((x Real)) (or (<= x GEN1) (>= x (+ c GEN2)))))
(assert (= BAV1 (forall ((x Real)) (or (<= x GEN1) (>= x (+ c GEN2))))))
(assert (= BAV2 (forall ((x Real)) (<= x GEN1))))
(assert (= BAV3 (forall ((x Real)) (>= x (+ c GEN2)))))
(check-sat)
(get-model)