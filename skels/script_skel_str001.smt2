(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-fun xx () String)
(declare-fun yy () String)
(declare-fun zz () String)
(declare-fun ww () String)
(declare-fun pp () String)
(declare-fun qq () String)
(assert (= (str.++ xx yy zz) (str.++ ww qq)))
(assert (= ww (str.++ xx pp)))
(assert (= yy pp))
(assert (not (= zz qq)))
(assert (= BAV1 (= (str.++ xx yy zz) (str.++ ww qq))))
(assert (= BAV2 (= ww (str.++ xx pp))))
(assert (= BAV3 (= yy pp)))
(assert (= BAV4 (= zz qq)))
(check-sat)