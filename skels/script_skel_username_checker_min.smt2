(set-info :smt-lib-version 2.5)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN4 String)
(declare-const GEN3 String)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(set-option :strings-exp true)
(declare-const buff String)
(declare-const pass_mem String)
(assert (= (str.len buff) GEN1))
(assert (= (str.len pass_mem) GEN2))
(assert (str.in.re (str.++ buff pass_mem) (re.+ (str.to.re GEN3))))
(assert (str.contains buff GEN4))
(assert (= BAV1 (= (str.len buff) GEN1)))
(assert (= BAV2 (= (str.len pass_mem) GEN2)))
(check-sat)
(get-model)