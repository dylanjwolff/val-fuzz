(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const GEN4 Int)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun a () Real)
(declare-fun b () Real)
(declare-fun c () Real)
(assert (and (= (+ (+ a b) c) GEN1) (= (+ (+ (* a b) (* b c)) (* c a)) GEN2) (= (- (* a (* b c)) GEN3) GEN4)))
(eliminate-quantifiers (exists b a))
(exit)
(assert (= BAV1 (and (= (+ (+ a b) c) GEN1) (= (+ (+ (* a b) (* b c)) (* c a)) GEN2) (= (- (* a (* b c)) GEN3) GEN4))))
(assert (= BAV2 (= (+ (+ a b) c) GEN1)))
(assert (= BAV3 (= (+ (+ (* a b) (* b c)) (* c a)) GEN2)))
(assert (= BAV4 (= (- (* a (* b c)) GEN3) GEN4)))