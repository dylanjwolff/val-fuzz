(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () Real)
(assert (and (< x GEN1) (>= (to_int x) GEN2)))
(assert (= BAV1 (and (< x GEN1) (>= (to_int x) GEN2))))
(assert (= BAV2 (< x GEN1)))
(assert (= BAV3 (>= (to_int x) GEN2)))
(check-sat)
(get-model)