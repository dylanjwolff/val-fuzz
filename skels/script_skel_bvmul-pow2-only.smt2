(set-logic ALL)
(declare-const GEN1 (_ BitVec 4))
(declare-const GEN2 (_ BitVec 4))
(declare-const GEN3 (_ BitVec 4))
(declare-const GEN4 (_ BitVec 4))
(declare-const GEN5 (_ BitVec 4))
(declare-const BAV6 Bool)
(declare-const BAV7 Bool)
(declare-const BAV8 Bool)
(declare-fun x () (_ BitVec 4))
(assert (= x GEN1))
(assert (= (bvmul (bvneg x) x) GEN2))
(assert (= (bvmul (bvneg GEN3) GEN4) GEN5))
(assert (= BAV6 (= x GEN1)))
(assert (= BAV7 (= (bvmul (bvneg x) x) GEN2)))
(assert (= BAV8 (= (bvmul (bvneg GEN3) GEN4) GEN5)))
(check-sat)