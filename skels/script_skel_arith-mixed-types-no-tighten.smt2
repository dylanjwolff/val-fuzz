(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () Real)
(declare-fun n () Int)
(assert (and (>= (+ x n) GEN1) (<= n GEN2) (<= x GEN3)))
(assert (= BAV1 (and (>= (+ x n) GEN1) (<= n GEN2) (<= x GEN3))))
(assert (= BAV2 (>= (+ x n) GEN1)))
(assert (= BAV3 (<= n GEN2)))
(assert (= BAV4 (<= x GEN3)))
(check-sat)
(get-model)