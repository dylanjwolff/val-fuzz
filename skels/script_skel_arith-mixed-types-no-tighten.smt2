(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const GEN3 Int)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(declare-const BAV7 Bool)
(declare-fun x () Real)
(declare-fun n () Int)
(assert (and (>= (+ x n) GEN1) (<= n GEN2) (<= x GEN3)))
(assert (= BAV4 (and (>= (+ x n) GEN1) (<= n GEN2) (<= x GEN3))))
(assert (= BAV5 (>= (+ x n) GEN1)))
(assert (= BAV6 (<= n GEN2)))
(assert (= BAV7 (<= x GEN3)))
(check-sat)
(get-model)