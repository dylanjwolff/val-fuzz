(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN4 Int)
(declare-const GEN3 Int)
(declare-const GEN2 String)
(declare-const GEN1 String)
(set-option :strings-exp true)
(declare-fun s () String)
(assert (str.contains s GEN1))
(declare-fun x () Int)
(assert (<= (str.len s) x))
(assert (not (= (str.indexof s GEN2 (- x GEN3)) (- GEN4))))
(assert (= BAV1 (<= (str.len s) x)))
(assert (= BAV2 (= (str.indexof s GEN2 (- x GEN3)) (- GEN4))))
(check-sat)
(get-model)