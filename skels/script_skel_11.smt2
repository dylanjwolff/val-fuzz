(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun a () Real)
(declare-fun b () Real)
(declare-fun r () Real)
(declare-fun s () Real)
(declare-fun t () Real)
(assert (and (= (+ (+ r s) t) GEN1) (= (- (+ (+ (* r s) (* s t)) (* t r)) a) GEN2) (= (- (* (* r s) t) b) GEN3)))
(eliminate-quantifiers (exists a b))
(exit)
(assert (= BAV1 (and (= (+ (+ r s) t) GEN1) (= (- (+ (+ (* r s) (* s t)) (* t r)) a) GEN2) (= (- (* (* r s) t) b) GEN3))))
(assert (= BAV2 (= (+ (+ r s) t) GEN1)))
(assert (= BAV3 (= (- (+ (+ (* r s) (* s t)) (* t r)) a) GEN2)))
(assert (= BAV4 (= (- (* (* r s) t) b) GEN3)))