(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const GEN4 Int)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(set-info :smt-lib-version 2.0)
(declare-fun x () Real)
(declare-fun y () Real)
(declare-fun n () Real)
(assert (= (/ x n) GEN1))
(assert (= (/ y n) GEN2))
(assert (<= n GEN3))
(assert (>= n GEN4))
(assert (= BAV1 (= (/ x n) GEN1)))
(assert (= BAV2 (= (/ y n) GEN2)))
(assert (= BAV3 (<= n GEN3)))
(assert (= BAV4 (>= n GEN4)))
(check-sat)
(get-model)