(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-fun x () Real)
(declare-fun y () Real)
(declare-fun z () Real)
(declare-fun w () Real)
(assert (< x w))
(assert (> (exp x) (exp y)))
(assert (> (exp y) (exp z)))
(assert (> (exp z) (exp w)))
(assert (= BAV1 (< x w)))
(assert (= BAV2 (> (exp x) (exp y))))
(assert (= BAV3 (> (exp y) (exp z))))
(assert (= BAV4 (> (exp z) (exp w))))
(check-sat)
(get-model)