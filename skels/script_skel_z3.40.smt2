(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(set-option :auto-config true)
(set-option :produce-models true)
(declare-sort A)
(declare-sort B)
(declare-fun f (A) B)
(assert (forall ((x A)(y A)) (! (=> (= (f x) (f y)) (= x y)) :pattern ((f x) (f y)))))
(declare-const a1 A)
(declare-const a2 A)
(declare-const b B)
(assert (not (= a1 a2)))
(assert (= (f a1) b))
(assert (= (f a2) b))
(assert (= BAV1 (forall ((x A)(y A)) (=> (= (f x) (f y)) (= x y)))))
(assert (= BAV2 (forall ((x A)(y A)) (= (f x) (f y)))))
(assert (= BAV3 (forall ((x A)(y A)) (= x y))))
(assert (= BAV4 (= a1 a2)))
(assert (= BAV5 (= (f a1) b)))
(assert (= BAV6 (= (f a2) b)))
(check-sat)
(get-model)