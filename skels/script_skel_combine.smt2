(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun a () Real)
(declare-fun b () Real)
(declare-fun c () Real)
(assert (> c GEN1))
(assert (> (* a b) GEN2))
(assert (< (* a b c) GEN3))
(assert (= BAV1 (> c GEN1)))
(assert (= BAV2 (> (* a b) GEN2)))
(assert (= BAV3 (< (* a b c) GEN3)))
(check-sat)
(get-model)