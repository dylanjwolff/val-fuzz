(set-logic ALL)
(declare-const GEN1 (_ BitVec 1))
(declare-const GEN2 (_ BitVec 5))
(declare-const GEN3 (_ BitVec 10))
(declare-const GEN4 (_ BitVec 1))
(declare-const GEN5 (_ BitVec 5))
(declare-const GEN6 (_ BitVec 10))
(declare-const BAV7 Bool)
(declare-const BAV8 Bool)
(declare-const BAV9 Bool)
(set-info :source "Handcrafted by CM Wintersteiger")
(define-sort FPN () (_ FloatingPoint 5 11))
(declare-fun x () FPN)
(assert (= x (fp GEN1 GEN2 GEN3)))
(declare-fun r () FPN)
(assert (= r (fp.roundToIntegral roundNearestTiesToEven x)))
(assert (not (= r (fp GEN4 GEN5 GEN6))))
(assert (= BAV7 (= x (fp GEN1 GEN2 GEN3))))
(assert (= BAV8 (= r (fp.roundToIntegral roundNearestTiesToEven x))))
(assert (= BAV9 (= r (fp GEN4 GEN5 GEN6))))
(check-sat)
(get-model)
(check-sat-using smt)
(check-sat-using (then
                     fpa2bv
                     (using-params simplify :elim_and true)
                     bit-blast
                     sat))
(exit)