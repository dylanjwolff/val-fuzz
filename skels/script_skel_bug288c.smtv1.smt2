(set-option :incremental false)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN4 Int)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () Int)
(declare-fun y () Int)
(declare-fun z () Int)
(assert (= BAV1 (and (= z GEN1) (>= (+ (- (* GEN2 x) (* GEN3 y)) z) GEN4))))
(assert (= BAV2 (= z GEN1)))
(assert (= BAV3 (>= (+ (- (* GEN2 x) (* GEN3 y)) z) GEN4)))
(check-sat-assuming ((and (= z GEN1) (>= (+ (- (* GEN2 x) (* GEN3 y)) z) GEN4))))
(get-model)