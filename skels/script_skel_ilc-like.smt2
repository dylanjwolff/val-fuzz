(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN3 Int)
(declare-const GEN2 String)
(declare-const GEN1 String)
(set-option :strings-exp true)
(declare-fun s () String)
(assert (= s GEN1))
(declare-fun target () String)
(assert (= target GEN2))
(declare-fun location () Int)
(assert (= location (str.indexof s target GEN3)))
(assert (= BAV1 (= s GEN1)))
(assert (= BAV2 (= target GEN2)))
(assert (= BAV3 (= location (str.indexof s target GEN3))))
(check-sat)
(get-model)