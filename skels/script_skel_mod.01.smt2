(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN1 Int)
(set-info :smt-lib-version 2.0)
(declare-fun n () Int)
(declare-fun x () Int)
(assert (>= n GEN1))
(assert (< (mod x n) n))
(assert (= BAV1 (>= n GEN1)))
(assert (= BAV2 (< (mod x n) n)))
(check-sat)
(get-model)