(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () Real)
(assert (and (< GEN1 x) (< x GEN2) (is_int x)))
(assert (= BAV1 (and (< GEN1 x) (< x GEN2) (is_int x))))
(assert (= BAV2 (< GEN1 x)))
(assert (= BAV3 (< x GEN2)))
(check-sat)
(get-model)