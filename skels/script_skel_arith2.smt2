(set-logic ALL)
(declare-const GEN1 Int)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(set-info :smt-lib-version 2.0)
(set-info :category "crafted")
(declare-fun v1 () Int)
(declare-fun v2 () Int)
(declare-fun v3 () Real)
(assert (= (+ v1 v2) v3))
(assert (< v3 v2))
(assert (> v3 (- v2 GEN1)))
(assert (= BAV2 (= (+ v1 v2) v3)))
(assert (= BAV3 (< v3 v2)))
(assert (= BAV4 (> v3 (- v2 GEN1))))
(check-sat)
(get-model)
(exit)