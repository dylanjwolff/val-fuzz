(set-option :smt.arith.solver 6)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const GEN3 Int)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(set-option :rewriter.push_to_real false)
(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun c () Int)
(assert (forall ((d Int)) (= (/ GEN1 (- GEN2 d (ite (= d a) b c))) GEN3)))
(assert (= BAV4 (forall ((d Int)) (= (/ GEN1 (- GEN2 d (ite (= d a) b c))) GEN3))))
(assert (= BAV5 (forall ((d Int)) (= d a))))
(check-sat)