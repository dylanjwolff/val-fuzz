(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(set-option :smt.mbqi true)
(declare-sort A)
(declare-sort B)
(declare-fun f (A) B)
(assert (forall ((b B)(x A)(y A)) (or (= x y) (not (= (f x) b)))))
(declare-fun a1 () A)
(declare-fun a2 () A)
(declare-fun x () B)
(assert (not (= a1 a2)))
(assert (= BAV1 (forall ((b B)(x A)(y A)) (or (= x y) (not (= (f x) b))))))
(assert (= BAV2 (forall ((b B)(x A)(y A)) (= x y))))
(assert (= BAV3 (forall ((b B)(x A)(y A)) (= (f x) b))))
(assert (= BAV4 (= a1 a2)))
(check-sat)
(get-model)