(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(declare-const BAV7 Bool)
(declare-const GEN6 Int)
(declare-const GEN5 Int)
(declare-const GEN4 Int)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun P (Int Int) Bool)
(declare-fun a () Int)
(assert (> a GEN1))
(assert (forall ((x Int)(y Int)) (=> (and (<= a x) (<= x (+ a GEN2)) (<= GEN3 y) (<= y (+ GEN4 x))) (P x y))))
(assert (not (P GEN5 GEN6)))
(assert (= BAV1 (> a GEN1)))
(assert (= BAV2 (forall ((x Int)(y Int)) (=> (and (<= a x) (<= x (+ a GEN2)) (<= GEN3 y) (<= y (+ GEN4 x))) (P x y)))))
(assert (= BAV3 (forall ((x Int)(y Int)) (and (<= a x) (<= x (+ a GEN2)) (<= GEN3 y) (<= y (+ GEN4 x))))))
(assert (= BAV4 (forall ((x Int)(y Int)) (<= a x))))
(assert (= BAV5 (forall ((x Int)(y Int)) (<= x (+ a GEN2)))))
(assert (= BAV6 (forall ((x Int)(y Int)) (<= GEN3 y))))
(assert (= BAV7 (forall ((x Int)(y Int)) (<= y (+ GEN4 x)))))
(check-sat)
(get-model)