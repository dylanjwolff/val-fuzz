(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-sort U 0)
(declare-fun a () U)
(declare-fun P (U U) Bool)
(assert (forall ((x U)) (= x a)))
(assert (forall ((x U)(y U)) (! (= x a) :pattern ((= x y)))))
(declare-fun b () U)
(declare-fun c () U)
(assert (not (= b c)))
(assert (= BAV1 (forall ((x U)) (= x a))))
(assert (= BAV2 (forall ((x U)(y U)) (= x a))))
(assert (= BAV3 (forall ((x U)(y U)) (= x y))))
(assert (= BAV4 (= b c)))
(check-sat)