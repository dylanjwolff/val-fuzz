(set-option :incremental false)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-fun x () (_ BitVec 32))
(declare-fun y () (_ BitVec 32))
(declare-fun z () (_ BitVec 32))
(assert (= x y))
(assert (= y z))
(assert (= BAV1 (= x y)))
(assert (= BAV2 (= y z)))
(assert (= BAV3 (= x z)))
(check-sat-assuming ((not (= x z))))
(get-model)