(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(set-info :smt-lib-version 2.0)
(set-info :category "industrial")
(declare-sort U 0)
(declare-fun a () U)
(declare-fun b () U)
(declare-fun f (U) U)
(declare-fun p () Bool)
(assert (and (= a b) (forall ((x U)) (=> (and (= (f x) a) (not (= (f x) b))) p))))
(assert (= BAV1 (and (= a b) (forall ((x U)) (=> (and (= (f x) a) (not (= (f x) b))) p)))))
(assert (= BAV2 (= a b)))
(assert (= BAV3 (forall ((x U)) (=> (and (= (f x) a) (not (= (f x) b))) p))))
(assert (= BAV4 (forall ((x U)) (and (= (f x) a) (not (= (f x) b))))))
(assert (= BAV5 (forall ((x U)) (= (f x) a))))
(assert (= BAV6 (forall ((x U)) (= (f x) b))))
(check-sat)
(exit)