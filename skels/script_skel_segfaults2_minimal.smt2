(set-option :auto-config false)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(set-option :smt.mbqi false)
(declare-fun ff ((Array Int Int)) (Array Int Int))
(assert (forall ((s (Array Int Int))) (! (= s (ff s)) :pattern (= s (ff s)))))
(apply simplify)
