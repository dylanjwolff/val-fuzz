(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-sort U 0)
(declare-fun f (U) U)
(declare-fun a () U)
(declare-fun b () U)
(assert (forall ((x (-> U U))) (= (x a) b)))
(assert (not (= (f a) b)))
(assert (= BAV1 (forall ((x (-> U U))) (= (x a) b))))
(assert (= BAV2 (= (f a) b)))
(check-sat)
(get-model)