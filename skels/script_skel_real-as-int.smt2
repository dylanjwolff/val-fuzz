(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun a () Real)
(declare-fun b () Real)
(assert (= (* a a) GEN1))
(assert (= (* b b) GEN2))
(assert (= (+ (* a a) (* b b)) GEN3))
(assert (= BAV1 (= (* a a) GEN1)))
(assert (= BAV2 (= (* b b) GEN2)))
(assert (= BAV3 (= (+ (* a a) (* b b)) GEN3)))
(check-sat)
(get-model)