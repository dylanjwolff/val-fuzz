(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const GEN3 Int)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(declare-fun a () Real)
(declare-fun b () Real)
(assert (= (* a a) GEN1))
(assert (= (* b b) GEN2))
(assert (= (+ (* a a) (* b b)) GEN3))
(assert (= BAV4 (= (* a a) GEN1)))
(assert (= BAV5 (= (* b b) GEN2)))
(assert (= BAV6 (= (+ (* a a) (* b b)) GEN3)))
(check-sat)