(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-fun f (Int) Int)
(declare-fun a () Int)
(declare-fun b () Int)
(assert (forall ((X Int)) (< X (f X))))
(assert (forall ((X Int)) (> (+ a b) (f X))))
(assert (= BAV1 (forall ((X Int)) (< X (f X)))))
(assert (= BAV2 (forall ((X Int)) (> (+ a b) (f X)))))
(check-sat)