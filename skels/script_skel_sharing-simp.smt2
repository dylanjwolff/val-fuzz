(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-fun x () Int)
(declare-fun y () Int)
(declare-fun A () (Set Int))
(declare-fun B () (Set Int))
(declare-fun C () (Set Int))
(declare-fun D () (Set Int))
(assert (member x A))
(assert (member y B))
(assert (or (= C (intersection A B)) (= D (intersection A B))))
(assert (= BAV1 (or (= C (intersection A B)) (= D (intersection A B)))))
(assert (= BAV2 (= C (intersection A B))))
(assert (= BAV3 (= D (intersection A B))))
(check-sat)
(get-model)