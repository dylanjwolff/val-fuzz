(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-codatatypes ((list 0)) (((cons (head Int) (tail list)))))
(declare-fun x () list)
(declare-fun y () list)
(assert (= x (cons GEN1 (cons GEN2 x))))
(assert (= y (cons GEN3 y)))
(assert (not (= x y)))
(assert (= BAV1 (= x (cons GEN1 (cons GEN2 x)))))
(assert (= BAV2 (= y (cons GEN3 y))))
(assert (= BAV3 (= x y)))
(check-sat)
(get-model)