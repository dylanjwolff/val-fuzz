(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const GEN6 (_ BitVec 23))
(declare-const GEN5 (_ BitVec 8))
(declare-const GEN4 (_ BitVec 1))
(declare-const GEN3 (_ BitVec 23))
(declare-const GEN2 (_ BitVec 8))
(declare-const GEN1 (_ BitVec 1))
(set-info :source "Handcrafted by CM Wintersteiger")
(define-sort FPN () (_ FloatingPoint 8 24))
(declare-const m FPN)
(assert (= m (fp.rem (fp GEN1 GEN2 GEN3) (fp #b0 GEN5 GEN6))))
(set-option :pp.fp_real_literals true)
(assert (= BAV1 (= m (fp.rem (fp GEN1 GEN2 GEN3) (fp #b0 GEN5 GEN6)))))
(check-sat)
(get-model)
(get-value (m))
(check-sat-using smt)
(get-value (m))
(check-sat-using (then fpa2bv smt))
(get-value (m))
(exit)