(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN2 Real)
(declare-const GEN1 String)
(simplify (str.to.int ""))
(simplify (str.to.int "-2"))
(simplify (str.to.int "2"))
(simplify (str.to.int "2/1"))
(simplify (int.to.str -1))
(simplify (int.to.str 1))
(simplify (int.to.str 0))
(simplify (str.to.int (int.to.str -2)))
(declare-fun x () Int)
(simplify (str.to.int (int.to.str x)))
(assert (= x (str.to.int GEN1)))
(assert (not (= x GEN2)))
(assert (= BAV1 (= x (str.to.int GEN1))))
(assert (= BAV2 (= x GEN2)))
(check-sat)
(get-model)