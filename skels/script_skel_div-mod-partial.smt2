(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN1 Int)
(declare-fun x () Int)
(declare-fun y () Int)
(assert (not (= y GEN1)))
(assert (not (= (- y (* (div y x) x)) (mod y x))))
(assert (= BAV1 (= y GEN1)))
(assert (= BAV2 (= (- y (* (div y x) x)) (mod y x))))
(check-sat)
(get-model)