(set-info :smt-lib-version 2.5)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN4 Int)
(declare-const GEN3 Int)
(declare-const GEN2 String)
(declare-const GEN1 String)
(set-option :strings-exp true)
(set-option :strings-fmf true)
(set-option :produce-models true)
(declare-fun x () String)
(assert (= (str.len (str.replaceall x GEN1 GEN2)) (+ (str.len x) GEN3)))
(assert (> (str.len x) GEN4))
(assert (= BAV1 (= (str.len (str.replaceall x GEN1 GEN2)) (+ (str.len x) GEN3))))
(assert (= BAV2 (> (str.len x) GEN4)))
(check-sat)
(get-model)