(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () Int)
(assert (<= x GEN1))
(assert (> (abs x) GEN2))
(assert (= BAV1 (<= x GEN1)))
(assert (= BAV2 (> (abs x) GEN2)))
(check-sat)
(get-model)
(get-value (x (abs x)))