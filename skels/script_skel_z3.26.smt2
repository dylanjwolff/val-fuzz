(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const BAV6 Bool)
(declare-const BAV7 Bool)
(declare-const GEN5 (_ BitVec 4))
(declare-const GEN4 (_ BitVec 4))
(declare-const GEN3 (_ BitVec 4))
(declare-const GEN2 (_ BitVec 4))
(declare-const GEN1 (_ BitVec 4))
(set-option :auto-config true)
(set-option :produce-models true)
(define-fun is-power-of-two ((x (_ BitVec 4))) Bool 
  (= #x0 (bvand x (bvsub x #x1))))
(declare-const a (_ BitVec 4))
(assert (not (= (is-power-of-two a) (or (= a GEN1) (= a GEN2) (= a GEN3) (= a GEN4) (= a GEN5)))))
(assert (= BAV1 (= (is-power-of-two a) (or (= a GEN1) (= a GEN2) (= a GEN3) (= a GEN4) (= a GEN5)))))
(assert (= BAV2 (or (= a GEN1) (= a GEN2) (= a GEN3) (= a GEN4) (= a GEN5))))
(assert (= BAV3 (= a GEN1)))
(assert (= BAV4 (= a GEN2)))
(assert (= BAV5 (= a GEN3)))
(assert (= BAV6 (= a GEN4)))
(assert (= BAV7 (= a GEN5)))
(check-sat)
(get-model)