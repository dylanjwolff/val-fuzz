(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN1 Int)
(declare-fun x () Real)
(assert (and (<= x GEN1) (> x (to_int x))))
(assert (= BAV1 (and (<= x GEN1) (> x (to_int x)))))
(assert (= BAV2 (<= x GEN1)))
(assert (= BAV3 (> x (to_int x))))
(check-sat)
(get-model)
(get-value (x (to_int x) (abs x)))