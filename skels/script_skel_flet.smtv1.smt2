(set-option :incremental false)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-sort U 0)
(declare-fun a () Bool)
(declare-fun b () Bool)
(assert (= BAV1 (and (and a b) (or (not a) (not b)))))
(assert (= BAV2 (and a b)))
(assert (= BAV3 (or (not a) (not b))))
(check-sat-assuming ((and (and a b) (or (not a) (not b)))))
(get-model)