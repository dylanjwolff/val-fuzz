(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(set-option :produce-models true)
(declare-fun x () Int)
(assert (>= GEN1 x))
(assert (<= GEN2 x))
(minimize x)
(maximize (+ x x))
(set-option :opt.priority pareto)
(assert (= BAV1 (>= GEN1 x)))
(assert (= BAV2 (<= GEN2 x)))
(check-sat)
(get-model)