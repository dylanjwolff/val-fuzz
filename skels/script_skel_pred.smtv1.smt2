(set-option :incremental false)
(set-info :category "crafted")
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-sort U 0)
(declare-fun x () U)
(declare-fun y () U)
(declare-fun f (U) Bool)
(assert (= BAV1 (and (f x) (= (f x) (f y)) (not (f y)))))
(assert (= BAV2 (= (f x) (f y))))
(check-sat-assuming ((and (f x) (= (f x) (f y)) (not (f y)))))
(get-model)