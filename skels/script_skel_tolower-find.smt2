(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const GEN4 String)
(declare-const GEN3 String)
(declare-const GEN2 String)
(declare-const GEN1 String)
(set-option :strings-exp true)
(declare-const x String)
(declare-const y String)
(assert (= (str.tolower x) GEN1))
(assert (= (str.tolower y) GEN2))
(assert (not (= x GEN3)))
(assert (not (= y GEN4)))
(assert (not (= x y)))
(assert (= BAV1 (= (str.tolower x) GEN1)))
(assert (= BAV2 (= (str.tolower y) GEN2)))
(assert (= BAV3 (= x GEN3)))
(assert (= BAV4 (= y GEN4)))
(assert (= BAV5 (= x y)))
(check-sat)
(get-model)