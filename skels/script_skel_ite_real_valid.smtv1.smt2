(set-option :incremental false)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN4 Real)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () Real)
(declare-fun b () Bool)
(assert (= BAV1 (=> (= x (/ (ite b GEN1 GEN2) GEN3)) (>= x GEN4))))
(assert (= BAV2 (= x (/ (ite b GEN1 GEN2) GEN3))))
(assert (= BAV3 (>= x GEN4)))
(check-sat-assuming ((not (=> (= x (/ (ite b GEN1 GEN2) GEN3)) (>= x GEN4)))))
(get-model)