(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-sort A 0)
(declare-sort B 0)
(declare-fun x () A)
(declare-fun y () A)
(declare-fun f (A) B)
(assert (not (=> (= x y) (= (f x) (f y)))))
(assert (= BAV1 (=> (= x y) (= (f x) (f y)))))
(assert (= BAV2 (= x y)))
(assert (= BAV3 (= (f x) (f y))))
(check-sat)
(get-model)