(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const GEN3 String)
(declare-const GEN2 String)
(declare-const GEN1 String)
(declare-fun x () String)
(declare-fun y () String)
(assert (str.in_re x (re.diff (re.* (str.to_re "A")) re.none)))
(assert (or (not (str.in_re x (re.* (str.to_re GEN2)))) (str.in_re y (re.diff (re.* (str.to_re GEN3)) re.all))))
(assert (= BAV1 (or (not (str.in_re x (re.* (str.to_re GEN2)))) (str.in_re y (re.diff (re.* (str.to_re GEN3)) re.all)))))
(check-sat)
(get-model)