(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN1 Int)
(declare-fun c () Int)
(declare-fun a () Int)
(declare-fun b () Int)
(assert (> a GEN1))
(assert (= c (* a b)))
(assert (not (= b (div c a))))
(assert (= BAV1 (> a GEN1)))
(assert (= BAV2 (= c (* a b))))
(assert (= BAV3 (= b (div c a))))
(check-sat)
(get-model)