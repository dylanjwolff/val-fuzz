(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN1 Int)
(declare-fun x () Real)
(assert (forall ((y Real)) (=> (> y GEN1) (< x y))))
(assert (= BAV1 (forall ((y Real)) (=> (> y GEN1) (< x y)))))
(assert (= BAV2 (forall ((y Real)) (> y GEN1))))
(assert (= BAV3 (forall ((y Real)) (< x y))))
(check-sat)
(get-model)