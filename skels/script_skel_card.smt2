(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN3 Int)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-sort E 0)
(declare-fun s () (Set E))
(declare-fun t () (Set E))
(assert (>= (card s) GEN1))
(assert (>= (card t) GEN2))
(assert (<= (card (union s t)) GEN3))
(assert (= BAV1 (>= (card s) GEN1)))
(assert (= BAV2 (>= (card t) GEN2)))
(assert (= BAV3 (<= (card (union s t)) GEN3)))
(check-sat)
(get-model)