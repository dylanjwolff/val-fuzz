(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-fun x () Int)
(declare-fun y () Int)
(declare-fun S () (Set Int))
(declare-fun T () (Set Int))
(assert (member x S))
(assert (= S (union T (singleton y))))
(assert (not (= x y)))
(assert (not (member x T)))
(assert (= BAV1 (= S (union T (singleton y)))))
(assert (= BAV2 (= x y)))
(check-sat)
(get-model)