(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(declare-fun x () String)
(declare-fun f () String)
(declare-fun y () Int)
(assert (= (str.len f) GEN1))
(assert (= (str.indexof x f GEN2) y))
(assert (= BAV1 (= (str.len f) GEN1)))
(assert (= BAV2 (= (str.indexof x f GEN2) y)))
(check-sat)
(get-model)