(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 String)
(declare-const GEN3 String)
(declare-const GEN4 Int)
(declare-const GEN5 Int)
(declare-const GEN6 Int)
(declare-const BAV7 Bool)
(declare-const BAV8 Bool)
(declare-fun a () String)
(assert (>= (str.len a) GEN1))
(assert (str.in.re a (re.+ (re.range GEN2 GEN3))))
(assert (<= GEN4 (str.to.int (str.substr a (+ (- GEN5) (str.len a)) GEN6))))
(assert (= BAV7 (>= (str.len a) GEN1)))
(assert (= BAV8 (<= GEN4 (str.to.int (str.substr a (+ (- GEN5) (str.len a)) GEN6)))))
(check-sat)
(get-model)