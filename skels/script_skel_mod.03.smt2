(set-logic ALL)
(declare-const GEN1 Int)
(declare-const GEN2 Int)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(set-info :smt-lib-version 2.0)
(declare-fun n () Int)
(declare-fun x () Int)
(assert (< (mod x n) GEN1))
(assert (< (div x n) GEN2))
(assert (= BAV3 (< (mod x n) GEN1)))
(assert (= BAV4 (< (div x n) GEN2)))
(check-sat)
(get-model)