(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN2 Int)
(declare-const GEN1 Int)
(set-info :smt-lib-version 2.0)
(declare-fun n () Int)
(declare-fun x () Int)
(assert (< (mod x n) GEN1))
(assert (< (div x n) GEN2))
(assert (= BAV1 (< (mod x n) GEN1)))
(assert (= BAV2 (< (div x n) GEN2)))
(check-sat)
(get-model)