(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN3 String)
(declare-const GEN2 String)
(declare-const GEN1 String)
(declare-fun x () String)
(assert (= (str.rev (str.toupper x)) GEN1))
(assert (not (= x GEN2)))
(assert (not (= x GEN3)))
(assert (= BAV1 (= (str.rev (str.toupper x)) GEN1)))
(assert (= BAV2 (= x GEN2)))
(assert (= BAV3 (= x GEN3)))
(check-sat)
(get-model)