(set-option :incremental false)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const GEN1 Int)
(declare-fun v2 () Real)
(declare-fun v1 () Real)
(declare-fun v0 () Real)
(assert (= BAV1 (<= (ite (>= (- v1) v0) v1 GEN1) v2)))
(assert (= BAV2 (>= (- v1) v0)))
(check-sat-assuming ((<= (ite (>= (- v1) v0) v1 GEN1) v2)))
(get-model)