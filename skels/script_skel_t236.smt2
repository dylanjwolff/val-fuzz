(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const BAV4 Bool)
(declare-const BAV5 Bool)
(declare-const GEN1 Int)
(declare-const x Real)
(declare-const a Int)
(declare-const b Int)
(declare-const c Int)
(declare-const d Int)
(declare-const e Int)
(assert (is_int x))
(assert (= (div a b) c))
(assert (= (mod a b) d))
(assert (= (div a b) e))
(assert (= (div a b) (to_int x)))
(assert (> (rem a b) GEN1))
(apply purify-arith)
(assert (= BAV1 (= (div a b) c)))
(assert (= BAV2 (= (mod a b) d)))
(assert (= BAV3 (= (div a b) e)))
(assert (= BAV4 (= (div a b) (to_int x))))
(assert (= BAV5 (> (rem a b) GEN1)))