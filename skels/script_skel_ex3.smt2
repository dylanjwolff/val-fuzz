(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(set-info :smt-lib-version 2.0)
(set-info :category "industrial")
(declare-sort U 0)
(declare-fun a () U)
(declare-fun f (U) U)
(assert (and (not (= (f (f a)) a)) (forall ((x U)) (= (f x) x))))
(assert (= BAV1 (and (not (= (f (f a)) a)) (forall ((x U)) (= (f x) x)))))
(assert (= BAV2 (= (f (f a)) a)))
(assert (= BAV3 (forall ((x U)) (= (f x) x))))
(check-sat)
(exit)