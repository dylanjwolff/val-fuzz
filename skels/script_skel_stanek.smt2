(set-option :produce-models true)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-const GEN2 (_ BitVec 16))
(declare-const GEN1 (_ BitVec 16))
(declare-fun q () (_ BitVec 16))
(declare-fun q1 () (_ BitVec 16))
(declare-fun q2 () (_ BitVec 16))
(define-fun bitxor ((x (_ BitVec 16))) (_ BitVec 16)
   (bvxor (bvshl x #x0008) x))
(assert (= q1 GEN1))
(assert (= q2 (bvxor (bvshl q1 GEN2) q1)))
(assert (= q (bitxor q1)))
(assert (= BAV1 (= q1 GEN1)))
(assert (= BAV2 (= q2 (bvxor (bvshl q1 GEN2) q1))))
(assert (= BAV3 (= q (bitxor q1))))
(check-sat)
(get-model)
(get-value (q1 q2 q))