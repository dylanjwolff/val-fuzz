(set-option :incremental false)
(set-logic ALL)
(declare-const BAV1 Bool)
(declare-const BAV2 Bool)
(declare-const BAV3 Bool)
(declare-sort U 0)
(declare-sort A 0)
(declare-sort B 0)
(declare-fun f (A) B)
(declare-fun x () A)
(declare-fun y () A)
(assert (= BAV1 (=> (= x y) (= (f x) (f y)))))
(assert (= BAV2 (= x y)))
(assert (= BAV3 (= (f x) (f y))))
(check-sat-assuming ((not (=> (= x y) (= (f x) (f y))))))
(get-model)